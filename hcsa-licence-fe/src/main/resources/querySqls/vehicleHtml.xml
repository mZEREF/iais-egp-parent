<?xml version="1.0" encoding="UTF-8"?>
<sqls catalog="vehicle">
    <sql key="generateVehicleHtml" cached="no" remarks="generate Vehicle Html">
        <![CDATA[
            <div class="form-horizontal vehicleContent">
                <input type="hidden" class="isPartEdit" name="isPartEdit${vehicleSuffix}" value="1"/>
                <input type="hidden" name="vehicleIndexNo${vehicleSuffix}" value=""/>
                <div class="form-group">
                    <div class="col-sm-7 col-md-6 col-xs-6">
                        <strong>Vehicle <label class="assign-psn-item">${vehicleLength}</label></strong>
                    </div>
                    <div class="col-sm-7 col-md-6 col-xs-6 text-right vehicleRemoveBtn">
                        <h4 class="text-danger">
                            <em class="fa fa-times-circle del-size-36 removeBtn cursorPointer"></em>
                        </h4>
                    </div>
                    <div class="clear"></div>
                </div>

                 <div class="form-group">
                    <label class="col-xs-5 col-md-5 col-sm-10 control-label control-font-label">Vehicle Number&nbsp;</label>
                    <div class="col-sm-7 col-md-7 col-xs-7">
                      <input class="vehicleName" type="text" name="vehicleName${vehicleSuffix}" value="" maxlength="10" autocomplete="off">
                      <span id="error_vehicleName${vehicleSuffix}" name="iaisErrorMsg" class="error-msg"></span>
                    </div>
                    <div class="clear"></div>
                 </div>

                 <div class="form-group">
                    <label class="col-xs-5 col-md-5 col-sm-10 control-label control-font-label">Chassis Number&nbsp;<span class="mandatory">*</span></label>
                    <div class="col-sm-7 col-md-7 col-xs-7">
                      <input class="chassisNum" type="text" name="chassisNum${vehicleSuffix}" value="" maxlength="25" autocomplete="off">
                      <span id="error_chassisNum${vehicleSuffix}" name="iaisErrorMsg" class="error-msg"></span>
                    </div>
                    <div class="clear"></div>
                 </div>
                 <div class="form-group">
                    <label class="col-xs-5 col-md-5 col-sm-10 control-label control-font-label">Engine Number&nbsp;<span class="mandatory">*</span></label>
                    <div class="col-sm-7 col-md-7 col-xs-7">
                      <input class="engineNum" type="text" name="engineNum${vehicleSuffix}" value="" maxlength="25" autocomplete="off">
                      <span id="error_engineNum${vehicleSuffix}" name="iaisErrorMsg" class="error-msg"></span>
                    </div>
                    <div class="clear"></div>
                 </div>

                <div class="col-md-12 col-xs-12">
                    <hr>
                </div>
            </div>
        ]]>
    </sql>
</sqls>


