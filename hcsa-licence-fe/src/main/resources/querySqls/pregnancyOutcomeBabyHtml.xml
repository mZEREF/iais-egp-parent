<?xml version="1.0" encoding="UTF-8"?>
<sqls catalog="pregnancyOutcomeStage">
    <sql key="generatePregnancyOutcomeBabyHtml" cached="no" remarks="generate Pregnancy Outcome Baby Html">
        <![CDATA[
            <div class="pregnancyOutcomeStageBabySection" id="pregnancyOutcomeStageBabySection${babyIndex}">
                <div class="form-group">
                    <label class="col-xs-5 col-md-6 control-label">Baby ${babyDisplayNum} Birth Weight</label>
                    <div class="col-sm-7 col-md-6 col-xs-7 col-md-6">
                        <select class="birthWeight" name="birthWeight${babyIndex}" id="birthWeight${babyIndex}">
                              <option value="">Please Select</option>
                              ${babyWeightSelectOptions}
                        </select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="col-xs-5 col-md-6 control-label">Baby ${babyDisplayNum} Birth Defect</label>
                    <div class="col-md-6">
                        <div class="form-check col-12">
                            <input class="form-check-input birthDefect"
                                   type="radio"
                                   name="birthDefect${babyIndex}"
                                   value="Yes"
                                   id="birthDefect${babyIndex}Yes"
                                   aria-invalid="false">
                            <label class="form-check-label"
                                   for="birthDefect${babyIndex}Yes"><span
                                    class="check-circle"></span>Yes</label>
                        </div>
                        <div class="form-check col-12">
                            <input class="form-check-input birthDefect"
                                   type="radio"
                                   name="birthDefect${babyIndex}"
                                   value="No"
                                   id="birthDefect${babyIndex}No"
                                   checked
                                   aria-invalid="false">
                            <label class="form-check-label"
                                   for="birthDefect${babyIndex}No"><span
                                    class="check-circle"></span>No</label>
                        </div>
                        <div class="form-check col-12">
                            <input class="form-check-input birthDefect"
                                   type="radio"
                                   name="birthDefect${babyIndex}"
                                   value="Unknown"
                                   id="birthDefect${babyIndex}Unknown"
                                   aria-invalid="false">
                            <label class="form-check-label"
                                   for="birthDefect${babyIndex}Unknown"><span
                                    class="check-circle"></span>Unknown</label>
                        </div>
                    </div>
                </div>
                <div name="defectTypeSectionName" style="display:none;">
                    <div class="form-group">
                        <label class="col-xs-5 col-md-6 control-label">Baby ${babyDisplayNum} Defect Type
                            &nbsp;<span class="mandatory">*</span>
                        </label>
                        <div class="col-sm-7 col-md-6 col-xs-7 col-md-6">
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT001"
                                       id="defectType${babyIndex}POSBDT001"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT001"><span
                                        class="check-square"></span>Chromosomal Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT002"
                                       id="defectType${babyIndex}POSBDT002"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT002"><span
                                        class="check-square"></span>Heart Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT003"
                                       id="defectType${babyIndex}POSBDT003"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT003"><span
                                        class="check-square"></span>Musculoskeletal Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT004"
                                       id="defectType${babyIndex}POSBDT004"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT004"><span
                                        class="check-square"></span>Nervous System Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT005"
                                       id="defectType${babyIndex}POSBDT005"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT005"><span
                                        class="check-square"></span>Other Fetal Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT006"
                                       id="defectType${babyIndex}POSBDT006"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT006"><span
                                        class="check-square"></span>Respiratory System Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT007"
                                       id="defectType${babyIndex}POSBDT007"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT007"><span
                                        class="check-square"></span>Urinary System Anomalies</label>
                            </div>
                            <div class="form-check col-xs-12">
                                <input class="form-check-input defectType" type="checkbox"
                                       name="defectType${babyIndex}"
                                       value="POSBDT008"
                                       id="defectType${babyIndex}POSBDT008"
                                       aria-invalid="false">
                                <label class="form-check-label"
                                       for="defectType${babyIndex}POSBDT008"><span
                                        class="check-square"></span>Others</label>
                            </div>
                        </div>
                    </div>
                    <div name="otherDefectTypeDivName" style="display:none;">
                        <div class="form-group">
                            <label class="col-xs-5 col-md-6 control-label">Baby ${babyDisplayNum} Defect Type (Others)
                                &nbsp;<span class="mandatory">*</span>
                            </label>
                            <div class="col-sm-7 col-md-6 col-xs-7 col-md-6">
                                <input type="text" class="otherDefectType" id="otherDefectType${babyIndex}" name="otherDefectType${babyIndex}" maxlength="20" autocomplete="off">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        ]]>
    </sql>
</sqls>


