<div class="row">
    <div class="col-xs-2 col-md-3">
        <h2 class="component-title">Master Code View</h2>
    </div>
    <div class="col-xs-10 col-md-6">
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" id="headingOne" role="tab">
                    <h2 class="panel-title"><a role="button" data-toggle="collapse" class="collapsed" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">Search Master Code:</a></h2>
                </div>
                <div class="panel-collapse collapse" id="collapseOne" role="tabpanel" aria-labelledby="headingOne">
                    <div class="panel-body">
                        <div class="panel-main-content">
                            <div class="col-xs-12 col-md-12">
                                <form class="form-horizontal">
                                    <div class="form-group">
                                        <label class="col-xs-12 col-md-5 control-label" for="category">Code Category</label>
                                        <div class="col-xs-12 col-md-7">
                                            <input id="category" type="text">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-xs-12 col-md-5 control-label" for="description">Code Description</label>
                                        <div class="col-xs-12 col-md-7">
                                            <input id="description" type="text">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-xs-12 col-md-5 control-label" for="esd">Effective Start Date</label>
                                        <div class="col-xs-12 col-md-7">
                                            <input id="esd" type="text">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-xs-12 col-md-5 control-label" for="eed">Effective End Date</label>
                                        <div class="col-xs-12 col-md-7">
                                            <input id="eed" type="text">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-xs-12 col-md-5 control-label" for="status">Status</label>
                                        <div class="col-xs-12 col-md-7">
                                            <input id="status" type="text">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-xs-1 col-md-1">
                                            <div class="components">
                                                <a class="btn btn-primary click">Primary</a>
                                            </div>
                                        </div>
                                        <div class="col-xs-offset-9 col-md-offset-7">
                                            <div class="components">
                                                <a class="btn btn-primary click">Clear</a>
                                            </div>
                                        </div>
                                    </div>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>