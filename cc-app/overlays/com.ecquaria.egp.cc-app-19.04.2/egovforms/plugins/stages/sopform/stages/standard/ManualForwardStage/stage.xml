<StageType>
	<Type>sopform.stages.standard.ManualForwardStage</Type>
	<Require>sopform.stages.base.BaseStage</Require>
	
	<Name>Manual Forward</Name>
	<Description>Send for manual forward</Description>
	<Icon>images/manualForward.png</Icon>
	<Javascript>javascripts/ManualForwardStage.js</Javascript>
	<Process>SOPFORMS:ManualForwardStage</Process>
	<Properties>
		<Property>
			<Name>stageType</Name>
			<Label>Stage Type</Label>
			<Value>Manual Forward</Value>
			<Type>label</Type>
		</Property>
		<Property>
			<Name>stageName</Name>
			<Label>Stage Name</Label>
			<Type>text</Type>
		</Property>
		<Property>
			<Type>separator</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>taskAssignment</Name>
			<Label>Task Assignment</Label>
			<Type>taskAssignment</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Type>separator</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>allowComment</Name>
			<Label>Allows Comment</Label>
			<Type>checkbox</Type>
			<Default>checked</Default>
		</Property>
		<Property>
			<Name>customizedAckPage</Name>
			<Label>Customized Acknowledgement Page</Label>
			<Type>customizedAckPage</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>customizedButtons</Name>
			<Label>Customized Buttons</Label>
			<Type>customizedButtons</Type>
			<Special>true</Special>
		</Property>
		
		<Property>
			<Type>separator</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>useOnPageLoad</Name>
			<Label>On Page Load</Label>
			<Type>checkbox</Type>
		</Property>
		<Property>
			<Name>processSelectionOPL</Name>
			<Label>Process Selection</Label>
			<Type>processSelectionOPL</Type>
			<Special>true</Special>
		</Property>
		
		<Property>
			<Type>separator</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>useOnPageSubmit</Name>
			<Label>On Page Submit</Label>
			<Type>checkbox</Type>
		</Property>
		<Property>
			<Name>processSelectionOPS</Name>
			<Label>Process Selection</Label>
			<Type>processSelectionOPS</Type>
			<Special>true</Special>
		</Property>
		
		<Property>
			<Type>separator</Type>
			<Special>true</Special>
		</Property>
		<Property>
			<Name>notifyTaskViaEmail</Name>
			<Label>Notify Task via Email</Label>
			<Type>checkbox</Type>
		</Property>
	</Properties>
	<Results>
	</Results>
</StageType>