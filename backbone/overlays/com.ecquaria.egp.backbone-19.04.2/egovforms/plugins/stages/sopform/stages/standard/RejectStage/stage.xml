<StageType>
	<Type>sopform.stages.standard.RejectStage</Type>
	<Require>sopform.stages.base.BaseStage</Require>
	
	<Name>Rejected</Name>
	<Description>Stage denoting that submission is rejected by officer</Description>
	<Icon>images/rejected.png</Icon>
	<Javascript>javascripts/RejectStage.js</Javascript>
	<Process>SOPFORMS:RejectedStage</Process>
	<Properties>
		<Property>
			<Name>stageType</Name>
			<Label>Stage Type</Label>
			<Value>Reject</Value>
			<Type>label</Type>
		</Property>
		<Property>
			<Name>stageName</Name>
			<Label>Stage Name</Label>
			<Type>text</Type>
		</Property>
		<Property>
			<Name>notifyApplicant</Name>
			<Label>Notify Applicant</Label>
			<Type>checkbox</Type>
		</Property>
		<Property>
			<Name>emailApplicant</Name>
			<Label>Email Applicant</Label>
			<Type>checkbox</Type>
		</Property>
	</Properties>
</StageType>