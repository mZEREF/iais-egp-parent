    <sql key="retrieveAllMasterCodes" remarks="To get all the masterCodes">
        <![CDATA[
          SELECT master_code_id, master_code_key, code_category, code_value, code_description, filter_value,
          sequence, status, effective_from, effective_to, version
          FROM CM_MASTER_CODE
          WHERE 1 = 1
